
#include <iostream>
#include <vector>

using namespace std;

int countOccurrences(vector<int>& arr, int key) {
    int left = 0, right = arr.size() - 1;
    int first_occurrence = -1;

    while (left <= right) {
        int mid = left + (right - left) / 2;
        if (arr[mid] == key) {
            first_occurrence = mid;
            right = mid - 1;
        } else if (arr[mid] < key) {
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }

    if (first_occurrence == -1) {
        return 0;
    } else {
        int count = 1;
        int i = first_occurrence + 1;
        while (i < arr.size() && arr[i] == key) {
            count++;
            i++;
        }
        return count;
    }
}

int main() {
    int T;
    cin >> T;

    while (T--) {
        int n;
        cin >> n;
        vector<int> arr(n);
        for (int i = 0; i < n; ++i) {
            cin >> arr[i];
        }
        int key;
        cin >> key;
        int occurrences = countOccurrences(arr, key);
        if (occurrences > 0) {
            cout << key << " " << occurrences << endl;
        } else {
            cout << "Key not present" << endl;
        }
    }

    return 0;
}
